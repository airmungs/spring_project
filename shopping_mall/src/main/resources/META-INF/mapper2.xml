<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sailmallDB">

<!-- 관리자 등록 신청 insert -->
<insert id="admin_insert" parameterType="shopping_admin.model.shopping_admin_dto">
INSERT INTO adminJoin (adid, adpass, adname, ademail, adtel, adpart, adposition, addate)
VALUES (#{adid}, #{adpass}, #{adname}, #{ademail}, #{adtel}, #{adpart}, #{adposition}, now())
</insert>

<!-- 관리자 등록 신청 ID중복체크 -->
<select id="admin_idcheck" parameterType="string" resultType="shopping_admin.model.shopping_admin_dto" >
SELECT FROM adminJoin WHERE adid=#{adid}
</select>

</mapper>
